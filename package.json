{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "build/bundle.js",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.server.js --progress",
    "dev:build-client": "cross-env NODE_ENV=development webpack --config webpack.client.js --progress",
    "dev:server": "nodemon .",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js --progress",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js --progress"
  },
  "author": "Victor Freitas <victorfreitasdev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "0.19.0",
    "compression": "1.7.4",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "express": "4.17.1",
    "express-http-proxy": "1.5.1",
    "helmet": "^3.18.0",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.0",
    "react-router-config": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
