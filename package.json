{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering React project",
  "main": "dist/server/index.js",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "webpack:server": "webpack --config webpack.server.js",
    "webpack:client": "webpack --config webpack.client.js",
    "dev:build-server": "cross-env NODE_ENV=development yarn webpack:server --progress",
    "dev:build-client": "cross-env NODE_ENV=development yarn webpack:client --progress",
    "dev:server": "nodemon .",
    "build:server": "cross-env NODE_ENV=production yarn webpack:server",
    "build:client": "cross-env NODE_ENV=production yarn webpack:client",
    "lint": "eslint src/**/*.{js,jsx}"
  },
  "author": "Victor Freitas <victorfreitasdev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "0.19.0",
    "compression": "1.7.4",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "4.17.1",
    "express-http-proxy": "1.6.0",
    "helmet": "^3.21.2",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.3",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "^0.13.3",
    "serialize-javascript": "2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
