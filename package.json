{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering React project",
  "main": "dist/server/index.js",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "precommit": "yarn lint",
    "build": "npm-run-all --parallel build:*",
    "webpack:server": "webpack --config webpack.server.js",
    "webpack:client": "webpack --config webpack.client.js",
    "dev:build-server": "cross-env NODE_ENV=development yarn webpack:server --progress",
    "dev:build-client": "cross-env NODE_ENV=development yarn webpack:client --progress",
    "dev:server": "nodemon .",
    "build:server": "cross-env NODE_ENV=production yarn webpack:server",
    "build:client": "cross-env NODE_ENV=production yarn webpack:client",
    "lint": "eslint src/**/*.{js,jsx}"
  },
  "author": "Victor Freitas <victorfreitasdev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "0.19.0",
    "compression": "1.7.4",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "express": "4.17.1",
    "express-http-proxy": "1.5.1",
    "helmet": "^3.18.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.0",
    "react-router-config": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "^0.13.2",
    "serialize-javascript": "1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^2.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
